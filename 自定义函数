#define _CRT_SECURE_NO_WARNINGS 1
#include <stdio.h>
int Add(int x,int y)
{
	int z = x + y;
	return z;
}
int main()
{
	int sum = 0; 
  { int num1 = 10;
	int num2 = 0;
	sum = Add(num1, num2);
	printf("sum=%d\n", sum);
	}
	{
	int a = 20;
	int b = 20;
	sum = Add(a, b);
	printf("sum=%d", sum);
     }
	return 0;
}
